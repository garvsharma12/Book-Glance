# Database
SUPABASE_ANON_KEY=anon_key
SUPABASE_SERVICE_ROLE=service_role
DATABASE_URL=supabase_url

# Admin Access
ADMIN_USERNAME=your_admin_username
ADMIN_PASSWORD_HASH=your_hashed_password

# API Keys
OPENAI_API_KEY=your_openai_key_here
# Google Vision API (any of the following are accepted)
GOOGLE_VISION_API_KEY=your_google_vision_key
# or
# GOOGLE_CLOUD_VISION_API_KEY=your_google_vision_key
# or
# VISION_API_KEY=your_google_vision_key
# or
# GOOGLE_API_KEY=your_google_vision_key
# or
# GCP_API_KEY=your_google_vision_key
# or
# GOOGLE_CLOUD_API_KEY=your_google_vision_key

# Gemini API (Google Generative AI)
# Either name is accepted
GOOGLE_GEMINI_API_KEY=your_gemini_api_key
# or
# GEMINI_API_KEY=your_gemini_api_key
# Optional: override default model
GEMINI_MODEL=gemini-1.5-flash

# Host information (for production)
HOST=bookglance.io

# Node Environment
NODE_ENV=development
